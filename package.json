{
  "name": "aggregate-sdk",
  "homepage": "https://aggregate.tibbo.com/",
  "version": "6.0.0-dev.20200910",
  "description": "AggreGate SDK (TypeScript, open-source) - Official Release",
  "scripts": {
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "build": "webpack --config webpack.dev.js --progress --noEmit && webpack --config webpack.prod.js --progress --noEmit",
    "dev": "webpack --config webpack.development.js --watch --progress && NODE_ENV=development",
    "test": "jest --config=jest.unit.config.js",
    "test:integration": "jest --testRegex=./test/integration/.*.test.ts --runInBand --testTimeout=30000",
    "test:performance": "jest --testRegex=./test/performance/.*.test.ts --runInBand --testTimeout=30000",
    "test:full": "npm run test && npm run test:integration",
    "test:coverage": "jest --coverage",
    "test:jenkins": "npm run test",
    "publish:next": "npm version 6.0.0-dev.20200910 && npm publish --tag next",
    "publish:last": "npm publish",
    "prettier": "prettier --write \"src/**/*.ts\" && prettier --write \"test/**/*.test.ts\"",
    "antrl4-js": "java -Xmx500M -cp '/usr/local/lib/antlr-4.7.2-complete.jar:$CLASSPATH' org.antlr.v4.Tool  -Dlanguage=JavaScript -lib src/expression/grammar -o src/expression/parser -visitor -no-listener -Xexact-output-dir src/expression/grammar/AggregateExpression.g4"
  },
  "keywords": [
    "tibbo",
    "aggregate",
    "sdk",
    "api"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "lint-staged": {
    "*.(js|ts)": "eslint"
  },
  "bugs": {
    "url": "https://github.com/Tibbo-Systems/aggregate-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tibbo-Systems/aggregate-sdk.git"
  },
  "files": [
    "README.md",
    "dist/aggregate-sdk.development.js",
    "dist/aggregate-sdk.production.min.js",
    "index.js",
    "types"
  ],
  "main": "index.js",
  "types": "./types/index.d.ts",
  "author": {
    "name": "Andrey Plis",
    "email": "andrey.plis1@gmail.com",
    "url": "https://github.com/AndreyPlis"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/antlr4": "^4.7.2",
    "@types/bytebuffer": "^5.0.41",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/pino": "^6.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "jest-html-reporter": "^3.2.0",
    "jest-junit": "^11.1.0",
    "lint-staged": "^10.3.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3"
  },
  "dependencies": {
    "antlr4": "^4.8.0",
    "bytebuffer": "^5.0.1",
    "i18next": "^19.7.0",
    "jsbi": "^3.1.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.27.0",
    "pino": "^6.6.0",
    "unescape-js": "^1.1.4",
    "uuid": "^8.3.0"
  }
}
