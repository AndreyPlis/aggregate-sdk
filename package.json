{
  "name": "aggregate-sdk",
  "homepage": "https://aggregate.tibbo.com/",
  "version": "5.70.16-dev.20200601",
  "description": "AggreGate SDK (TypeScript, open-source) - Official Release",
  "scripts": {
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts}' --quiet --fix",
    "build": "webpack --config webpack.dev.js --progress --noEmit && webpack --config webpack.prod.js --progress --noEmit",
    "dev": "webpack --config webpack.development.js --watch --progress",
    "test": "jest",
    "test:integration": "jest --testRegex=./test/integration/.*.ts --testPathIgnorePatterns=[] --runInBand --testTimeout=30000",
    "test:performance": "jest --testRegex=./test/performance/.*.ts --testPathIgnorePatterns=[] --runInBand --testTimeout=30000",
    "test:full": "npm run test && npm run test:integration",
    "test:coverage": "jest --coverage",
    "test:jenkins": "npm run test",
    "publish:next": "npm version 5.70.16-dev.20200601 && npm publish --tag next",
    "publish:last": "npm publish",
    "prettier": "prettier --write \"src/**/*.ts\" ",
    "antrl4-js": "java -Xmx500M -cp '/usr/local/lib/antlr-4.7.2-complete.jar:$CLASSPATH' org.antlr.v4.Tool  -Dlanguage=JavaScript -lib src/expression/grammar -o src/expression/parser -visitor -no-listener -Xexact-output-dir src/expression/grammar/AggregateExpression.g4"
  },
  "keywords": [
    "tibbo",
    "aggregate",
    "sdk",
    "api"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "lint-staged": {
    "*.(js|ts)": "eslint"
  },
  "bugs": {
    "url": "https://github.com/Tibbo-Systems/aggregate-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tibbo-Systems/aggregate-sdk.git"
  },
  "files": [
    "README.md",
    "dist/aggregate-sdk.development.js",
    "dist/aggregate-sdk.production.min.js",
    "index.js",
    "types"
  ],
  "main": "index.js",
  "types": "./types/index.d.ts",
  "contributors": [
    {
      "name": "Andrey Plis",
      "email": "andrey.plis@tibbo.com",
      "url": "https://github.com/AndreyPlis"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/antlr4": "^4.7.1",
    "@types/bytebuffer": "^5.0.41",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.154",
    "@types/pino": "^6.0.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-loader": "^8.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "fork-ts-checker-webpack-plugin": "^4.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-html-reporter": "^3.1.3",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.7",
    "moment-locales-webpack-plugin": "^1.2.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "antlr4": "^4.8.0",
    "bytebuffer": "^5.0.1",
    "i18next": "^19.4.5",
    "jsbi": "^3.1.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.26.0",
    "pino": "^6.3.1",
    "pino-pretty": "^4.0.0",
    "unescape-js": "^1.1.4",
    "uuid": "^8.1.0"
  }
}
